<p id="notice"><%= notice %></p>

<h1>Critical Asset List Devices</h1>

<table>
  <thead>
    <tr>
      <th>Special device list</th>
      <th>Device</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @special_device_list_devices.each do |special_device_list_device| %>
      <tr>
        <td><%= special_device_list_device.special_device_list_id %></td>
        <td><%= special_device_list_device.device_id %></td>
        <td><%= link_to 'Show', special_device_list_device %></td>
        <td><%= link_to 'Edit', edit_special_device_list_device_path(special_device_list_device) %></td>
        <td><%= link_to 'Destroy', special_device_list_device, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

